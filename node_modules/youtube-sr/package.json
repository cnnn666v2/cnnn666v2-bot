{
  "name": "youtube-sr",
  "version": "4.1.9",
  "description": "Simple package to make YouTube search.",
  "files": [
    "dist"
  ],
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    "require": "./dist/main.js",
    "import": "./dist/main.mjs"
  },
  "scripts": {
    "build": "rimraf dist && tsc && gen-esm-wrapper ./dist/main.js ./dist/main.mjs",
    "test": "tsc && cd dev && node index.js",
    "format": "prettier --write **/*.{js,ts}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevSnowflake/youtube-sr.git"
  },
  "keywords": [
    "youtube",
    "api",
    "search",
    "playlist",
    "channel",
    "video",
    "scrape",
    "ytsr",
    "ytpl",
    "yt-search",
    "youtube-search",
    "ytdl",
    "youtube-dl",
    "scrape-youtube",
    "youtube-scrape",
    "youtube-api",
    "simple-youtube-api"
  ],
  "author": "Snowflake107",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DevSnowflake/youtube-sr/issues"
  },
  "homepage": "https://github.com/DevSnowflake/youtube-sr#readme",
  "dependencies": {
    "node-fetch": "^2.6.5"
  },
  "devDependencies": {
    "@types/node": "^15.12.0",
    "fs-extra": "^10.0.0",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "^2.3.0",
    "readdirp": "^3.6.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
